    "# [4]\n",
    "\n",
    "idx = pd.Index([1,3,5,9,12], name='month')\n",
    "df = pd.DataFrame({'sale':[10, 30, 50, 40, 70],'stock':[100,100,200,200,300]},index=idx)\n",
    "df\n",
    "\n",
    "arr = np.where(df['sale']>=50, 'high', 'low')\n",
    "r4 = df.groupby(arr).groups\n",
    "r5 = df.groupby(arr).size()\n",
    "arr; r4; r5"
   ]
