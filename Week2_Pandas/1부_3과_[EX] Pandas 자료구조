    "# [1-1]\n",
    "\n",
    "df = pd.read_csv('data/01_03_01.csv')\n",
    "df\n",
    "sr1 = df['id']  # type : series\n",
    "sr2 = df.loc[0] # type : series\n",
    "\n",
    "print(sr1.name, sr1.values, sr1.index) # 'id' , [101,102,103,104] , RangeIndex(start=0, stop=4, step=1)\n",
    "print(sr2.name, sr2.values, sr2.index) # 0 , [101,kim,19] , Index(['id', 'name', 'age'], dtype='object')\n",
    "# sr1; sr2"
   ]
