    "### [예제7] 기본 Indexing 요약"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# [[], .at, .iat]\n",
    "\n",
    "file1 = 'data/02_02_01.csv'\n",
    "df = pd.read_csv(file1, index_col = 'names')\n",
    "df\n",
    "\n",
    "r0 = df.at['kim','kor']\n",
    "r1 = df['kor']['kim']\n",
    "r2 = df.iat[0,2]\n",
    "r3 = df['kor']\n",
    "r4 = df[['kor', 'mat']]\n",
    "r5 = df[0:3]\n",
    "r6 = df['kim':'park']\n",
    "r0; r1; r2; r4\n",
    "disp('r5', 'r6', 'r7')"
   ]
  },
