   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.DataFrame(np.arange(6).reshape(3,2),columns=['price','class'], index=['1st',2,'three'])\n",
    "df\n",
    "\n",
    "r1 = df.price\n",
    "r2 = df.class # error\n",
    "r3 = df['class']\n",
    "r4 = df.price.three\n",
    "r5 = df.price.1st # error\n",
    "r6 = df['price']['1st']\n",
    "r7 = df.loc['1st', 'price']\n",
    "r1; r3; r4; r6; r7"
   ]
